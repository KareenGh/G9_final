<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="770.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.ComplainReportController">
    <children>
        <BarChart fx:id="complain_barchart" layoutX="91.0" layoutY="58.0" prefHeight="414.0" prefWidth="589.0" title="complaints">
            <xAxis>
                <CategoryAxis fx:id="date" side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="amount" label="amount of complaints" side="LEFT" />
            </yAxis>
        </BarChart>
        <Button fx:id="back" layoutX="39.0" layoutY="33.0" mnemonicParsing="false" onAction="#back1" text="back" />
    </children>
</AnchorPane>
