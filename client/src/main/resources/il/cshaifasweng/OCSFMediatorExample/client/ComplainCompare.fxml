<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="770.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.ComplainCompareController">
    <children>
        <BarChart fx:id="complain_report1" layoutX="28.0" layoutY="65.0" prefHeight="421.0" prefWidth="357.0" title="complain report 1">
            <xAxis>
                <CategoryAxis fx:id="compalin1" side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="amount1" label="amount of complaints" side="LEFT" />
            </yAxis>
        </BarChart>
        <BarChart fx:id="complain_report2" layoutX="391.0" layoutY="58.0" prefHeight="421.0" prefWidth="357.0" title="complain report 2">
            <xAxis>
                <CategoryAxis fx:id="complain2" side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="amount2" label="amount of complaints" side="LEFT" />
            </yAxis>
        </BarChart>
        <Button fx:id="back" layoutX="28.0" layoutY="14.0" mnemonicParsing="false" onAction="#back1" text="back" />
    </children>
</AnchorPane>
